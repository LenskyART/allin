{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@babel/eslint-plugin",
        "prettier"
    ],
    "rules": {
        "require-jsdoc": "off"
//        "quotes": ["error", "single"]
    }
}
